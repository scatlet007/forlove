<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="shop.entity">
	<class name="Support" table="t_support">
		<id name="supportid" column="supportid"></id>
		<property name="sound" column="sound"></property>
		<property name="video" column="video"></property>
		<property name="picture" column="picture"></property>
		<property name="smarty" column="smarty"></property>
		<property name="business" column="business"></property>
		<property name="sellingpoint" column="sellingpoint"></property>
		<property name="phoneid" column="phoneid"></property>
	</class>
</hibernate-mapping>
