<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="shop.entity">
	<class name="Screen" table="t_screen">
		<id name="screenid" column="screenid"></id>
		<property name="screenType" column="screenType"></property>
		<property name="resolution" column="resolution"></property>
		<property name="size" column="size"></property>
		<property name="PPI" column="PPI"></property>
		<property name="texture" column="texture"></property>
		<property name="color" column="color"></property>
		<property name="phoneid" column="phoneid"></property>
	</class>
</hibernate-mapping>
