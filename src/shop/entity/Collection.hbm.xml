<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="shop.entity">
	<class name="Collection" table="t_collection">
		<id name="collectionid" column="collectionid"></id>
		<property name="phoneid" column="phoneid"></property>
		<property name="userid" column="userid"></property>
		<property name="tag" column="tag"></property>
	</class>
</hibernate-mapping>
